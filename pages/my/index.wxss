page{background: #FFF}
.avatar {
    padding: 90rpx 30rpx 70rpx;
    text-align: center;
}

.avatar .avatarimg {
    display: block;
    margin: 0 auto;
    width: 176rpx;
    height: 176rpx;
    border-radius:50%;
    overflow:hidden;
}

.avatar .changeName {
    margin-top: 30rpx;
}

.name {
    height: 96rpx;
    margin: 0 30rpx;
    border-bottom: 1rpx solid #e6e6e6;
}

.name .left {
    float: left;
    width: 120rpx;
    line-height: 96rpx;
}

.name .right {
    margin-left: 150rpx;
    line-height: 96rpx;
    height: 96rpx;
    position: relative;
    z-index: 1;
}

.name .icon-delete {
    float: right;
    position: relative;
    top: -96rpx;
    line-height: 96rpx;
    height: 96rpx;
    z-index: 2;
}
.jianjie {
    /* height: 26rpx; */
    margin: 0 30rpx;
    /* border-bottom: 1rpx solid #e6e6e6; */
    padding-bottom:80rpx;
    
}.kongge{height: 30rpx;}
.jianjie .left {
    float: left;
    width: 160rpx;
    line-height: 96rpx;
    font-size:34rpx
}
.shangchuanchangeName{color:#999;font-size:24rpx}
.jianjie .right {
    margin-left: 150rpx;
    line-height: 96rpx;
    height: 96rpx;
    position: relative;
    z-index: 1;
    font-size:32rpx
}

.jianjie .icon-delete {
    float: right;
    position: relative;
    top: -96rpx;
    line-height: 96rpx;
    height: 96rpx;
    z-index: 2;
}

.bottom {
    padding: 30rpx;
    position: fixed;
    bottom: 0;
    width: 100%;
    box-sizing: border-box;
    z-index:1000
}

.bottom .btn {
    height: 90rpx;
    line-height: 90rpx;
    width: 100%;
    border-radius: 90rpx;
    text-align: center;
    background-color: #8f2924;
    color:#FFF;
}
.page__bd{padding:30rpx}
.radio{padding-right:20rpx}
.gongpai{margin:30rpx;width:200rpx;height:100px;background:#f3f3f3}
.gongpai image {
display: block;
margin: 0 auto;
width: 176rpx;
height: 176rpx;
}

.house_plan_title {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx 30rpx;
}

.house_plan_text {
    font-size: 24rpx;
    color: #aeaeae;
}

.hou_plan_from {
    background-color: white;
    padding-left: 30rpx;
    font-size: 26rpx;
}

.general_style {
    display: flex;
    align-items: center;
    position: relative;
}

.general_style .red-tip {
    position: absolute;
    right: -14rpx;
    top: -10rpx;
}

.text-weight {
    font-weight: 700;
}

.hou_plan_item {
    height: 90rpx;
}

.hou_plan_live {
    border-bottom: 1px solid #e6e6e6;
    width: 522rpx;
    height: 100%;
    justify-content: space-between;
    padding-right: 30rpx;
    margin-top: 10rpx;
}

.hou_plan_live2 {
    border-bottom: 1px solid #e6e6e6;
    width: 552rpx;
    height: 100%;
    margin-top: 10rpx;
}

.general_style2 {
    display: flex;
}

.house_item {
    width: 70rpx;
    margin-top: 10rpx;
}

.general_style_font {
    padding-left: 14rpx;
}

.xian {
    height: 100%;
    display: flex;
    align-items: center;
    width: 552rpx;
    border-bottom: 1px solid #e6e6e6;
}

.add_pic {
    width: 102rpx;
    height: 102rpx;
    background-color: #f2f2f2;
    margin-top: 10rpx;
}

.pic_content {
    font-size: 24rpx;
    padding-bottom: 30rpx;
}

.general_style3 {
    display: flex;
    justify-content: center;
    align-items: center;
}

.shizi_style {
    transform: rotate(45deg);
}

.add_pic_tohouse {
    height: 82rpx;
    width: 100%;
    background-color: white;
    margin-top: 10rpx;
    font-size: 32rpx;
    font-weight: 700;
}

.add_submit {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 90rpx;
    background-color: white;
    z-index: 7;
}

.add_submit_item {
    width: 690rpx;
    height: 78rpx;
    background-color: #3772cc;
    border-radius: 8rpx;
}

.add_submit_text {
    font-size: 30rpx;
    color: white;
}

.upload-file {
    /* overflow: hidden; */
    float: left;
    margin-right:30rpx
}

.upload-file>view {
    float: left;
    margin-right: 5rpx;
}
.upload-file .pic {
    width: 204rpx;
    height: 204rpx;
    box-sizing: border-box;
    margin-top: 20rpx;
    border-radius: 4px;
    position: relative;
    margin-bottom: 30rpx;
    background: #f2f2f2 url(data:image/png;base64,) center / 20px no-repeat;
}
.upload-file .activepic{background: #f2f2f2 url(data:image/jpg;base64,)}
.upload-file .small-level-color {
    display: inline-block;
    width: 90rpx;
    text-align: center;
    font-size: 20rpx;
    margin-top: 4px;
}